# SCsub

Import('env')

module_env = env.Clone()
module_env.Append(CPPDEFINES=['HAVE_CONFIG_H'])
module_env.Prepend(CPPPATH=['#thirdparty/libsamplerate/src'])
module_env.Prepend(CPPPATH=['#modules/opus_stream/include'])
module_env.add_source_files(env.modules_sources, Glob("#modules/opus_stream/libsamplerate/src/*.c"))
module_env.add_source_files(env.modules_sources, "*.cpp")
